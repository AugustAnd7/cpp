#include<iostream>
using namespace std;
int main() {
//读取输入
	int arr[3][3];
	for(int i = 0 ; i < 3; i++){
		for (int j = 0; j < 3; j++)
		{
			cin >> arr[i][j];
		}
	}
//制表输出
	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 3; j++)
		{
			cout << arr[i][j] << "\t";
		}
		cout << endl;
	}
//指针输出
	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 3; j++)
		{
			cout << (int) & arr[i][j] << "\t";
		}
		cout << endl;
	}
	system("pause");
	
	return 0;
}
